#!/bin/bash

ssh -T Administrador@fenix 'bash -s' <<EOF

# Tribo@5506

cd /cygdrive/d/Web/sitetribo;
mkdir -p /cygdrive/d/Web/tmp.branch
touch /cygdrive/d/Web/tmp.branch/sitetribo.branch;

/bin/env -i /usr/bin/git branch --no-color 2> /dev/null | sed -e '/^[^*]/d' -e "s/* \(.*\)/\1/" > /cygdrive/d/Web/tmp.branch/sitetribo.branch;

# /bin/env -i /usr/bin/git stash
# /bin/env -i /usr/bin/git checkout master
# /bin/env -i /usr/bin/git reset --hard
# /bin/env -i /usr/bin/git pull origin master

/bin/env -i /cygdrive/d/Web/sitetribo/bin/branch;

echo "                                "
echo "< Deploy finalizado... Yeahh!! >"
echo "------------------------------  "
echo "        \   ^__^                "
echo "         \  (oo)\_______        "
echo "            (__)\       )\/\    "
echo "                ||----w |       "
echo "                ||     ||       "
echo "                                "

exit 0
EOF

exit 0